{
    "extends": "../tslint.json",
    "rules": {
        "no-console": false,
        "no-implicit-dependencies": [
            true,
            "dev"
        ],
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration",
            "variable-declaration",
            "member-variable-declaration"
        ],
        "no-unexternalized-strings": false
    }
}
